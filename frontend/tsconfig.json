{
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "@type/*": ["type/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"],
      "@svg": ["assets/svg/RootSvg.tsx"],
      "@color/*": ["assets/color/*"],
      "@redux/*": ["redux/*"],
      "@api/*": ["api"],
      "@hooks/*": ["hooks/*"],
      "@modules/*": ["modules/*"]
    },
    "types": ["@types/jest"],
    "target": "ES2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "redux/store.js"],
  "exclude": ["node_modules"]
}
